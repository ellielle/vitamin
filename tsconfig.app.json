{
  "extends": "@vue/tsconfig/tsconfig.web.json",
  "exclude": [
    "node_modules",
    "dist"
  ],
  "compilerOptions": {
    "composite": true,
    "baseUrl": ".",
    "module": "esnext",
    "target": "esnext",
    "lib": [
      "esnext",
      "dom"
    ],
    "allowJs": false,
    "esModuleInterop": false,
    "moduleResolution": "node",
    "strict": true,
    "noUnusedLocals": true,
    "strictNullChecks": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    //    "isolatedModules": true,
    //    "skipLibCheck": true,
    //    "resolveJsonModule": true,
    "types": [
      "vite",
      "client"
    ],
    "paths": {
      "@/*": [
        "./src/*"
      ]
    }
  }
}
